<div class="container">
  <div class="row">
    <div class="col  s12">
      <p>
        <strong>First name</strong>
        <%= @user.first_name %>
      </p>

      <p>
        <strong>Last name</strong>
        <%= @user.last_name %>
      </p>

      <p>
        <strong>Email</strong>
        <%= @user.email %>
      </p>

      <%= link_to 'Edit', edit_user_path(@user), class: 'btn  waves-effect  waves-light' %>
      <%= link_to 'Back', users_path, class: 'btn  waves-effect  waves-light' %>
    </div>
  </div>
</div>